          - visitDate

---
kind: BooleanExpressionType
version: v1
definition:
  name: SessionsBoolExp
  operand:
    object:
      type: Sessions
      comparableFields:
        - fieldName: id
          booleanExpressionType: Int8BoolExp
        - fieldName: visitDate
          booleanExpressionType: DateBoolExp
      comparableRelationships:
        - relationshipName: encounters
  logicalOperators:
    enable: true
  isNull:
    enable: true
  graphql:
    typeName: SessionsBoolExp

---
kind: AggregateExpression
version: v1
definition:
  name: SessionsAggExp
  operand:
    object:
      aggregatedType: Sessions
      aggregatableFields:
        - fieldName: id
          aggregateExpression: Int8AggExp
        - fieldName: visitDate
          aggregateExpression: DateAggExp
  count:
    enable: true
    returnType: Int64
  graphql:
    selectTypeName: SessionsAggExp

---
kind: OrderByExpression
version: v1
definition:
  name: SessionsOrderByExp
  operand:
    object:
      orderedType: Sessions
      orderableFields:
        - fieldName: id
          orderByExpression: Int64OrderByExp
        - fieldName: visitDate
          orderByExpression: DateOrderByExp
      orderableRelationships: []
  graphql:
    expressionTypeName: SessionsOrderByExp

---
kind: Model
version: v2
definition:
  name: Sessions
  objectType: Sessions
  source:
