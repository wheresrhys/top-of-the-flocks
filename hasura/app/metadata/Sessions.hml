    collection: Sessions
  filterExpressionType: SessionsBoolExp
  aggregateExpression: SessionsAggExp
  orderByExpression: SessionsOrderByExp
  graphql:
    selectMany:
      queryRootField: sessions
      subscription:
        rootField: sessions
    selectUniques:
      - queryRootField: sessionsById
        uniqueIdentifier:
