{
	"name": "top-of-the-flocks",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"db:sync:prod": "supabase migration up --linked --debug --include-all",
		"db:sync:local": "supabase db reset && npm run db:import:dev ./data/end25.csv",
		"db:diff": "supabase db diff --schema public --local",
		"db:local": "supabase start",
		"db:types": "supabase gen types typescript --local --schema public > ./types/supabase.types.ts",
		"db:import:prod": "./scripts/load-prod-env.sh tsx scripts/import-csv.ts",
		"db:import:local": "dotenvx run -f .env.dev -- tsx scripts/import-csv.ts",
		"start": "next start",
		"build": "next build --turbopack",
		"dev": "dotenvx run -f .env.dev -- next dev --turbopack",
		"build:debug": "dotenvx run -f .env.dev -- next build --turbopack --debug",
		"lint": "npx prettier -w app && npx eslint",
		"typecheck": "tsc --noEmit",
		"qa": "npm run lint && npm run typecheck",
		"prepare": "husky"
	},
	"dependencies": {
		"@iconify-json/fluent-emoji-flat": "^1.2.5",
		"@supabase/supabase-js": "^2.75.1",
		"@tailwindcss/postcss": "^4.1.18",
		"date-fns": "^4.1.0",
		"flyonui": "^2.4.1",
		"next": "^16.1.4",
		"p-ratelimit": "^1.0.1",
		"postcss": "^8.5.6",
		"prettier": "^3.7.4",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"tailwindcss": "^4.1.18"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "^1.52.0",
		"@eslint/eslintrc": "^3",
		"@iconify-json/tabler": "^1.2.26",
		"@iconify/tailwind4": "^1.2.1",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"csv-parser": "^3.2.0",
		"eslint": "^9",
		"eslint-config-next": "15.5.6",
		"husky": "^9.1.7",
		"supabase": "^2.72.8",
		"tsx": "^4.20.6",
		"typescript": "^5",
		"yaml": "^2.4.0"
	}
}
