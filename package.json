{
	"name": "top-of-the-flocks",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "./scripts/load-env.sh next dev --turbopack",
		"build": "./scripts/load-env.sh next build --turbopack",
		"build:debug": "./scripts/load-env.sh next build --turbopack --debug",
		"start": "next start",
		"lint": "npx prettier -w app && npx eslint",
		"supabase:types": "./scripts/load-env.sh echo 'beginning type generation' && npx supabase gen types typescript --project-id $(op run --no-masking --env-file=\".env.1password\" -- printenv SUPABASE_PROJECT_ID) --schema public > ./types/supabase.types.ts  && echo 'types regenerated ok' || echo 'run `npx supabase login` to sign in to supabase first'",
		"supabase:migrate": "supabase migration up --linked --debug --include-all",
		"supabase:sync": "npm run supabase:migrate && npm run supabase:types",
		"supabase:import": "./scripts/load-env.sh tsx scripts/import-csv.ts",
		"build:prod": "curl -X POST $(op run --no-masking --env-file=\".env.1password\" -- printenv VERCEL_DEPLOY_HOOK)",
		"env:load": "./scripts/load-env.sh",
		"env:check": "op whoami && echo '✅ 1Password CLI authenticated' || echo '❌ Please run: op signin'",
		"prepare": "husky"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.75.1",
		"@tailwindcss/postcss": "^4.1.18",
		"flyonui": "^2.4.1",
		"intl-dateformat": "^0.1.4",
		"next": "^16.1.4",
		"p-ratelimit": "^1.0.1",
		"postcss": "^8.5.6",
		"prettier": "^3.7.4",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"tailwindcss": "^4.1.18"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@iconify-json/tabler": "^1.2.26",
		"@iconify/tailwind4": "^1.2.1",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"csv-parser": "^3.2.0",
		"eslint": "^9",
		"eslint-config-next": "15.5.6",
		"husky": "^9.1.7",
		"supabase": "^2.72.8",
		"tsx": "^4.20.6",
		"typescript": "^5",
		"yaml": "^2.4.0"
	}
}
