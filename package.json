{
	"name": "top-of-the-flocks",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "./scripts/load-env.sh next dev --turbopack",
		"build": "./scripts/load-env.sh next build --turbopack",
		"start": "next start",
		"lint": "npx prettier -w app && npx eslint",
		"supabase:types": "./scripts/load-env.sh echo 'beginning type generation' && npx supabase gen types typescript --project-id $(op run --no-masking --env-file=\".env.1password\" -- printenv SUPABASE_PROJECT_ID) --schema public > ./types/supabase.types.ts  && echo 'types regenerated ok' || echo 'run `npx supabase login` to sign in to supabase first'",
		"supabase:update-views": "./scripts/supabase-update-views.sh",
		"hasura:update-graph": "./scripts/hasura-update-graph.sh",
		"hasura:restart": "./scripts/hasura-restart.sh",
		"hasura:function-config": "tsx scripts/generate-function-config.ts",
		"hasura:add-function": "./scripts/add-hasura-function.sh",
		"hasura:add-native-op": "tsx scripts/add-native-operation.ts",
		"supabase:sync": "npm run supabase:update-views && npm run hasura:update-graph && npm run hasura:restart && npm run hasura:types",
		"hasura:sync": "npm run hasura:update-graph && npm run hasura:restart && npm run hasura:types",
		"hasura:dev": "cd hasura && ddn console --local && ddn run docker-start",
		"hasura:types": "./scripts/load-env.sh npx graphql-codegen --config codegen.ts",
		"import": "./scripts/load-env.sh tsx scripts/import-csv.ts",
		"env:load": "./scripts/load-env.sh",
		"env:check": "op whoami && echo '✅ 1Password CLI authenticated' || echo '❌ Please run: op signin'"
	},
	"dependencies": {
		"@emotion/cache": "^11.14.0",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@mui/icons-material": "^7.3.4",
		"@mui/material": "^7.3.4",
		"@supabase/supabase-js": "^2.75.1",
		"csv-parser": "^3.2.0",
		"intl-dateformat": "^0.1.4",
		"next": "^15.5.9",
		"p-ratelimit": "^1.0.1",
		"prettier": "^3.7.4",
		"react": "19.1.0",
		"react-dom": "19.1.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@graphql-codegen/cli": "^6.0.1",
		"@graphql-codegen/introspection": "^5.0.0",
		"@graphql-codegen/typescript": "^5.0.2",
		"@graphql-codegen/typescript-operations": "^5.0.2",
		"@graphql-tools/graphql-file-loader": "^8.1.2",
		"@graphql-tools/load": "^8.1.2",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.5.6",
		"graphql-tag": "^2.12.6",
		"supabase": "^2.53.6",
		"tsx": "^4.20.6",
		"typescript": "^5"
	}
}
