# 1Password Environment Variables Configuration
# This file maps environment variable names to 1Password secret references
# Format: VARIABLE_NAME=op://vault/item/field

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=op://TOTF/Supabase/url
NEXT_PUBLIC_SUPABASE_ANON_KEY=op://TOTF/Supabase/anon_key
SUPABASE_SERVICE_ROLE_KEY=op://TOTF/Supabase/service_role_key

# Hasura Configuration
# NEXT_PUBLIC_HASURA_ENDPOINT=op://TOTF/Hasura/endpoint
# HASURA_ADMIN_SECRET=op://TOTF/Hasura/admin_secret
HASURA_DDN_PROJECT_DIRECTORY=./app
APP_TOTF_CONNECTION_URI=op://TOTF/Hasura/supabaseConnectionUri
APP_TOTF_HASURA_CONNECTOR_PORT=6402
APP_TOTF_HASURA_SERVICE_TOKEN_SECRET=op://TOTF/Hasura/serviceTokenSecret
APP_TOTF_AUTHORIZATION_HEADER="BEARER ${APP_TOTF_HASURA_SERVICE_TOKEN_SECRET}"
APP_TOTF_OTEL_EXPORTER_OTLP_ENDPOINT="http://local.hasura.dev:4317"
APP_TOTF_OTEL_SERVICE_NAME="app_totf"
APP_TOTF_READ_URL="http://local.hasura.dev:6402"
APP_TOTF_WRITE_URL="http://local.hasura.dev:6402"
HASURA_DDN_PAT=op://TOTF/Hasura/privateAccessToken
